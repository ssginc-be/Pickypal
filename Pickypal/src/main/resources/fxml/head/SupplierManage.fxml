<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<!-- (본사) 납품업체 관리 -->
<VBox xmlns="http://javafx.com/javafx/21.0.5" xmlns:fx="http://javafx.com/fxml/1" fx:controller="common.NavController" prefHeight="700.0" prefWidth="800.0" BorderPane.alignment="CENTER">
    <children>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
                <DatePicker focusTraversable="false" layoutX="325.0" layoutY="20.0"/>
                <Button layoutX="751.0" layoutY="63.0" mnemonicParsing="false" text="Excel"/>
                <Button layoutX="689.0" layoutY="63.0" mnemonicParsing="false" text="Search"/>
                <Text layoutX="265.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="최종 수정일">
                    <font>
                        <Font name="SUIT Medium" size="12.0"/>
                    </font>
                </Text>
                <TextField layoutX="80.0" layoutY="21.0"/>
                <Text layoutX="30.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="납품처명">
                    <font>
                        <Font name="SUIT Medium" size="12.0"/>
                    </font>
                </Text>
                <TextField layoutX="582.0" layoutY="21.0"/>
                <Text layoutX="530.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="거래 상태">
                    <font>
                        <Font name="SUIT Medium" size="12.0"/>
                    </font>
                </Text>
            </children>
        </AnchorPane>
        <TableView fx:id="orderTable" prefHeight="700.0" prefWidth="806.0">
            <columns>
                <!-- 선택 (체크박스) 컬럼 -->
                <TableColumn fx:id="selectColumn" text="선택"/>
                <!-- 납품처 ID 컬럼 -->
                <TableColumn fx:id="supplierIdColumn" prefWidth="150" text="납품처 ID"/>
                <!-- 납품처명 컬럼 -->
                <TableColumn fx:id="supplierNameColumn" prefWidth="150" text="납품처명"/>
                <!--거래상태 컬럼 -->
                <TableColumn fx:id="statusColumn" prefWidth="150" text="거래상태"/>
                <!-- 주소 컬럼 -->
                <TableColumn fx:id="addressColumn" prefWidth="150" text="주소"/>
                <!-- 전화 번호 컬럼 -->
                <TableColumn fx:id="telColumn" text="전화 번호"/>
                <!-- 최종 수정일 컬럼 -->
                <TableColumn fx:id="lastModifiedAtColumn" text="최종 수정일"/>
            </columns>
        </TableView>
    </children>
</VBox>

