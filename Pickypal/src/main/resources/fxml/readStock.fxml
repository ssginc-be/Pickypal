<VBox fx:id="changePane" prefHeight="700.0" prefWidth="800.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
                <DatePicker focusTraversable="false" layoutX="80.0" layoutY="63.0" />
                <Button layoutX="751.0" layoutY="63.0" mnemonicParsing="false" text="Excel" />
                <Button layoutX="689.0" layoutY="63.0" mnemonicParsing="false" text="Delete" />
                <Text layoutX="30.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="등록일">
                    <font>
                        <Font name="SUIT Medium" size="12.0" />
                    </font>
                </Text>
                <TextField layoutX="80.0" layoutY="21.0" />
                <Text layoutX="30.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Test">
                    <font>
                        <Font name="SUIT Medium" size="12.0" />
                    </font>
                </Text>
                <TextField layoutX="349.0" layoutY="21.0" />
                <Text layoutX="302.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Test">
                    <font>
                        <Font name="SUIT Medium" size="12.0" />
                    </font>
                </Text>
                <TextField layoutX="562.0" layoutY="21.0" />
                <Text layoutX="528.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="주소">
                    <font>
                        <Font name="SUIT Medium" size="12.0" />
                    </font>
                </Text>
                <DatePicker focusTraversable="false" layoutX="350.0" layoutY="63.0" />
                <Text layoutX="303.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="수정일">
                    <font>
                        <Font name="SUIT Medium" size="12.0" />
                    </font>
                </Text>
            </children>
        </AnchorPane>
        <TableView fx:id="stockTable" prefHeight="700.0" prefWidth="806.0">
            <columns>
                <!-- 선택 (체크박스) 컬럼 -->
                <TableColumn fx:id="selectColumn" text="선택" />
                <!-- 재고 ID 컬럼 -->
                <TableColumn fx:id="stockIdColumn" text="재고 ID" />
                <!-- 상품 ID 컬럼 -->
                <TableColumn fx:id="itemIdColumn" text="상품 ID" />
                <!-- 카테고리 컬럼 -->
                <TableColumn fx:id="categoryColumn" text="카테고리" />
                <!-- 납품처 컬럼 -->
                <TableColumn fx:id="supplierColumn" text="납품처" />
                <!-- 상품명 컬럼 -->
                <TableColumn fx:id="nameColumn" text="상품명" />
                <!-- 가격 컬럼 -->
                <TableColumn fx:id="priceColumn" text="가격" />
                <!-- 태그 컬럼 -->
                <TableColumn fx:id="tagColumn" text="태그" />
                <!-- 보유 수량 컬럼 -->
                <TableColumn fx:id="stockColumn" text="보유 수량" />
                <!-- 등록일 컬럼 -->
                <TableColumn fx:id="createAtColumn" text="등록일" />
                <!-- 수정일 컬럼 -->
                <TableColumn fx:id="updatedAtColumn" text="수정일" />
            </columns>
        </TableView>
    </children>
</VBox>
