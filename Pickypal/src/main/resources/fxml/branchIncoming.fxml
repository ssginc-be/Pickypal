                                                <Image url="@../../../../../MiniProject/src/main/resources/img/LogoExample.png" />
                                            </image></ImageView>
                                    </graphic>
                                </Button>
                                <Button onAction="#branchManageButtonClick" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="발주 관리" textFill="WHITE">
