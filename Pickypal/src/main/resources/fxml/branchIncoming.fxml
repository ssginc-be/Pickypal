                                                <Image url="@../icon/stockInquery.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                    <font>
                                        <Font name="SUIT Light" size="16.0" />
                                    </font>
                                </Button>
                                <Button onAction="#incomingRegistButtonClick" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="입고 관리" textFill="WHITE">
