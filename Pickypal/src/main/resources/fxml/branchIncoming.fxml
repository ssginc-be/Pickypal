                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../../../../../MiniProject/src/main/resources/img/LogoExample.png" />
                                            </image></ImageView>
                                    </graphic>
                                </Button>
                            </children>
